#include <stdio.h>
#define MAXLINE  100

int i, c, len, x = 0;
char sa[MAXLINE + 1];
char pa[MAXLINE + 1];

int
main ()
{
  printf ("enter string\n");

  while ((c = getchar ()) != '\n')
    {

      sa[i++] = c;

    }
  
  i=0;
  printf ("enter search characters\n");
  while ((c = getchar ()) != '\n')
    {
      pa[i++] = c;
    }
 
 for (x=0; x<MAXLINE && len ==0 ; x++) 
 {
 while (len==0)
 {
     for (i=0; i<MAXLINE && len == 0 ; i++)
     
     if (sa[i] == pa[x])
     len++;
 }
 } 
  if (len !=0)
  printf("\nHIT!!!\n");
  return 0;
}
